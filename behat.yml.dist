default:
    suites:
        default:
            contexts:
                - App\Tests\Acceptance\ReportContext:
                    kernel: '@kernel'
                    importerService: '@App\Service\ImporterService'
                - App\Tests\Acceptance\ImportContext:
                    kernel: '@kernel'

    extensions:
        Behat\Symfony2Extension:
            kernel:
                bootstrap: vendor/autoload.php
                class: App\Kernel
