{% extends "base.html.twig" %}
{% block title %}Suite{% endblock %}
{% block page_title %}Suite: {{ uuid }}{% endblock %}
{% block body %}
    {% for subTitle, table in report %}
    <h3>{{ subTitle }}</h3>
    <table class="ui table">
        <thead>
        <tr>
            <th>Subject</th>
            <th>Min</th>
            <th>Max</th>
            <th>Mean</th>
            <th>Mode</th>
            <th>R-stdev</th>
        </tr>
        </thead>
        <tbody>
        {% for variant in table %}
        <tr>
            <td>{{ variant['subject-name'] }}</td>
            <td>{{ variant['stats-min'] | number_format('2') }}µs</td>
            <td>{{ variant['stats-max'] | number_format('2') }}µs</td>
            <td>{{ variant['stats-mean'] | number_format('2') }}µs</td>
            <td>{{ variant['stats-mode'] | number_format('2') }}µs</td>
            <td>{{ variant['stats-stdev'] | number_format('2') }}%</td>
        </tr>
        {% endfor %}
        </tbody>
    </table>
    {% endfor %}
{% endblock %}
