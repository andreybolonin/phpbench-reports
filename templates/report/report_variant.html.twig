{% extends "base.html.twig" %}
{% block title %}Variant{% endblock %}
{% block subtitle %}So many datas{% endblock %}
{% block breadcrumb %}
    {{ breadcrumb([
        {
            "label": project.namespace,
            "route": "report_namespace",
            "params": {
                namespace: project.namespace
            }
        },
        {
            "label": project.name,
            "route": "report_project",
            "params": {
                namespace: project.namespace,
                project: project.name,
            }
        },
        {
            "label": uuid,
            "route": "report_suite",
            "params": {
                namespace: project.namespace,
                project: project.name,
                uuid: uuid 
            }
        },
        {
            "label": class | short_class,
            "route": "report_benchmark",
            "params": {
                namespace: project.namespace,
                project: project.name,
                class: class,
                uuid: uuid 
            }
        },
        {
            "label": subject,
        },
        {
            "label": "#" ~ variant,
        },
    ]) }}
{% endblock %}
{% block body %}

    <h3>Iterations</h3>
    {% include "report/chart/iterations_chart.html.twig" with {
        chartName: "iterationChart",
        chart: iterationChart,
    } %}

    <h3>Distribution</h3>
    {% include "report/chart/iterations_histogram.html.twig" with {
        chartName: "histogramChart",
        chart: histogramChart,
    } %}

    <h3>Table</h3>
    {% include "report/table/iterations.html.twig" %}

{% endblock %}
